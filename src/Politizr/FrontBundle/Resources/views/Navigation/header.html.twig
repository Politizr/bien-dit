<div id="header">
    <div class="headerLogo">
        <a href="{{ path('Homepage') }}">
            <i class="iconPltzrLogo"></i>
        </a>
    </div>
    <div id="menu">
        <div class="pad40">
            {% if is_granted('ROLE_CITIZEN_INSCRIPTION') or is_granted('ROLE_ELECTED_INSCRIPTION') %}
                {% include 'PolitizrFrontBundle:Navigation:menuInscription.html.twig' %}
            {% else %}
                {% include 'PolitizrFrontBundle:Navigation:menuPublic.html.twig' %}
            {% endif %}
        </div>
    </div>
    
    <div id="social">
        <div class="pad40">
            {% include 'PolitizrFrontBundle:Navigation:share.html.twig' with { 'uri': app.request.uri } %}
        </div>
    </div>
</div>
