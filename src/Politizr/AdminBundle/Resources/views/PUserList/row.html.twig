{% extends_admingenerated "PolitizrAdminBundle:PUserList:row.html.twig" %}

{% block list_td_column_file_name %}
    {{ PUser | photo('admin_list_thumb') }}
{% endblock list_td_column_file_name %}

{% block list_td_column_p_u_status_id %}
    {{ PUser.PUStatus }}
{% endblock list_td_column_p_u_status_id %}

{% block list_td_column_p_l_city %}
    {% if PUser.PLCityId %}
        {{ PUser.PLCity }} ({{ PUser.PLCity.PLDepartment }})
    {% endif %}
{% endblock list_td_column_p_l_city %}
