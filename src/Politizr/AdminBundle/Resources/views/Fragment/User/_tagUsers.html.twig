<div id="displayDebatesZone">
    {% if users | length > 0 %}
        <b>{{ users | length }} utilisateurs ont été taggés avec {{ tag }}</b>
    {% else %}
        <b>Aucun utilisateur taggé avec {{ tag }}</b>
    {% endif %}
    <hr>
    {% for user in users %}
        {% include 'PolitizrAdminBundle:Fragment\\User:_user.html.twig' with { 'user': user } %}
    {% endfor %}
</div>
