{
    "name": "studioecho-sf2/politizr",
    "license": "(c) Studio Echo",
    "type": "project",
    "description": "Studio Echo SF2 / Politizr",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": {
        "phing": {
            "type": "pear",
            "url": "http://pear.phing.info"
        },
        "phingGit": {
            "type": "package",
            "package": {
                "name": "phing/phing",
                "type": "component",
                "version": "master",
                "source": {
                    "url": "git://github.com/Xosofox/phing.git",
                    "type": "git",
                    "reference": "master"
                }                
            }
        },
        "medium-editor" : {
            "type": "package",
            "package": {
                "require": {
                    "robloach/component-installer": "*"
                },
                "name": "daviferreira/medium-editor",
                "type": "component",
                "version": "master",
                "source": {
                    "url": "git://github.com/daviferreira/medium-editor.git",
                    "type": "git",
                    "reference": "master"
                },                
                "extra": {
                    "component": {
                        "scripts": [
                            "dist/js/medium-editor.min.js"
                        ],
                        "styles": [
                            "dist/css/medium-editor.min.css",
                            "dist/css/themes/*.min.css"
                        ]
                    }
                }
            }
        },
        "classList" : {
            "type": "package",
            "package": {
                "require": {
                    "robloach/component-installer": "*"
                },
                "name": "eligrey/classList.js",
                "type": "component",
                "version": "master",
                "source": {
                    "url": "git://github.com/eligrey/classList.js.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "classList.min.js"
                        ]
                    }
                }
            }
        },
        "jscroll" : {
            "type": "package",
            "package": {
                "require": {
                    "robloach/component-installer": "*"
                },
                "name": "pklauzinski/jscroll",
                "type": "component",
                "version": "master",
                "source": {
                    "url": "git://github.com/pklauzinski/jscroll.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "jquery.jscroll.min.js"
                        ]
                    }
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.3",
        
        "symfony/symfony": "2.3.*",
        
        "twig/extensions": "1.0.*",
        
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
                
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",
        
        "friendsofsymfony/user-bundle": "*",
        
        "fzaninotto/faker": "1.4.*@dev",
        "willdurand/faker-bundle": "@stable",

        "willdurand/geocoder-bundle": "3.0.*",

        "hwi/oauth-bundle": "0.4.*@dev",
        "facebook/php-sdk-v4": "dev-master",
        
        "propel/propel-bundle": "1.2.*",
        "willdurand/propel-typehintable-behavior": "1.0.4",
        "craftyshadow/propel-equalnest-behavior": "~1.2",

        "phing/phing": "2.4.14",
        
        "white-october/pagerfanta-bundle": "1.0.*",
        
        "ensepar/html2pdf-bundle": "1.0.0",
        
        "elao/web-profiler-extra-bundle" : "~2.3@dev",

        "cedriclombardot/admingenerator-generator-bundle": "~1.1",
        "cedriclombardot/admingenerator-user-bundle": "~1.1",
        "avocode/form-extensions-bundle": "dev-master",
        "liuggio/ExcelBundle": "2.0.0",

        "liip/imagine-bundle": "0.11.*",

        "robloach/component-installer": "dev-master",
        "components/bootstrap": "v3.3.0",
        "fortawesome/font-awesome": "dev-master",
        "braincrafted/bootstrap-bundle": "dev-master",
        "components/bootbox": "dev-master",
        "malsup/form" : "dev-master",
        "daviferreira/medium-editor" : "dev-master",
        "eligrey/classList.js" : "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },    
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "web/components"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
